#include<iostream>
using namespace std;
int main(){
   int n,c,r,x;
   cout<<"enter the num:";
   cin>>n;
   c=0;
   x=n;
   while (n!=0){
    r=n%10;
    c=c*10+r;
    n/=10;
   }
   cout<<c<<endl;
   if (c==x){
    cout<<"the given num is palimdrome;";
   }else{
      cout<<"the given num is not palimdrome;";
   }
    return 0;
}